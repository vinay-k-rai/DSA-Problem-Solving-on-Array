class Solution {
public:
    vector<vector<int>> permute(vector<int>& nums) {
        vector<vector<int>> res;
        int fact_n = tgamma(nums.size() + 1);

        for(int i = 0; i < fact_n; i++){
            next_permutation(nums.begin(), nums.end());
            res.push_back(nums);
        }
        return res;
    }
};
