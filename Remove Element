class Solution {
public:
    int removeElement(vector<int>& nums, int val) {
        int n=nums.size();
        if(n==0) return 0;
        int left=0,right=n-1;
        while(left<=right)
        {
            
            if(nums[left]!=val) left++;
            else if(nums[right]==val) right--;
            else
            {
                swap(nums[left],nums[right]);
                left++;
                right--;
            }
        }
        return left;
    }
};
